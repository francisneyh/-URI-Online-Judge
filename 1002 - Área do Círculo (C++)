#include <iostream>
#include <iomanip>
#include <math.h>
 
using namespace std;
 
int main() {
 
    /**
     * Escreva a sua solução aqui
     * Code your solution here
     * Escriba su solución aquí
     */
     
    double n = 3.14159, raio, area;
     
    cin >> raio;
    
    area = n * pow (raio, 2);
     
    cout << "A=" << fixed << setprecision(4)  <<  area << endl; 
  
 
    return 0;
}
